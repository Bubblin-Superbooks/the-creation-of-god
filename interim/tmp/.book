{
  "page-9": "<img class=\"overlay center\" width = \"77.92445911257792%\" src=\"data:image/jpeg;base64,\" url = \"https://raw.githubusercontent.com/marvindanig/the-creation-of-god/master/assets/images/new-cover.jpg\" />",
